{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
     {
      "src": "style.css",
      "use": "@vercel/static"
    },
     {
      "src": "client.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/socket.io/",
      "dest": "server.js"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}